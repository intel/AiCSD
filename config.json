{
    "config": {
        "logging": {
            "C_LOG_LEVEL": "INFO",
            "PY_LOG_LEVEL": "INFO"
        },
        "cert_type": [
            "zmq"
        ],
        "pipelines": [
            {
                "name": "person_detection",
                "source": "image_ingestor",
                "queue_maxsize": 50,
                "pipeline": "appsrc name=source  ! decodebin ! videoconvert ! video/x-raw,format=BGR ! udfloader name=udfloader ! gvametaconvert add-empty-results=true name=metaconvert ! gvametapublish name=destination ! queue ! videoconvert ! jpegenc ! multifilesink location=/tmp/files/output/output.jpg",
                "parameters": {
                    "type": "object",
                    "properties": {
                        "udfloader": {
                            "element": {
                                "name": "udfloader",
                                "property": "config",
                                "format": "json"
                            },
                            "type": "object"
                        }
                    }
                },
                "auto_start": false,
                "publish_frame": true
            }
        ]
    },
    "interfaces": {
        "Publishers": [
            {
                "Name": "default",
                "Type": "zmq_ipc",
                "zmq_send_hwm": 50,
                "EndPoint": "/EII/sockets",
                "Topics": [
                    "person_detection"
                ],
                "AllowedClients": [
                    "*"
                ]
            }
        ]
    }
}